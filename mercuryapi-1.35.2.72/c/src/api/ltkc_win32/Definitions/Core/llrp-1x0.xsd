<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.llrp.org/ltk/schema/core/encoding/xml/1.0"
	xmlns="http://www.llrp.org/ltk/schema/core/encoding/xml/1.0"
	xmlns:llrp="http://www.llrp.org/ltk/schema/core/encoding/xml/1.0"
	elementFormDefault="qualified"
>

	<!-- Message header attribute group -->
	<xs:attributeGroup name="headerAttrs">
		<xs:attribute name="MessageID" type ="xs:unsignedInt" use="required"/>
		<xs:attribute name="Version" type="xs:unsignedShort" />

		<!-- overrides for negative testing only -->
		<xs:attribute name="Reserved" type ="xs:unsignedInt"/>
		<xs:attribute name="Length" type ="xs:unsignedShort"/>
		<xs:attribute name="TypeID" type ="xs:unsignedShort"/>
	</xs:attributeGroup>

	<!-- Parameter header attribute group -->
	<xs:attributeGroup name="paramAttrs">

		<!-- overrides for negative testing only -->
		<xs:attribute name="Reserved" type ="xs:unsignedInt"/>
		<xs:attribute name="Length" type ="xs:unsignedShort"/>
		<xs:attribute name="TypeID" type ="xs:unsignedShort"/>
	</xs:attributeGroup>

	<!-- Common datatypes -->
	<xs:simpleType name="listOfUnsignedShort">
		<xs:list itemType="xs:unsignedShort"/>
	</xs:simpleType>

	<xs:simpleType name="listOfUnsignedByte">
		<xs:list itemType="xs:unsignedByte"/>
	</xs:simpleType>

	<xs:simpleType name="listOfUnsignedInt">
		<xs:list itemType="xs:unsignedInt"/>
	</xs:simpleType>

	<xs:complexType name="bitVector">
		<xs:simpleContent>
			<xs:extension base="xs:hexBinary">
				<xs:attribute name="Count" type="xs:unsignedShort"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<xs:simpleType name="twoBit">
		<xs:restriction base="xs:unsignedInt">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="3"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="hexByte">
		<xs:restriction base="xs:hexBinary">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="hexDWord">
		<xs:restriction base="xs:hexBinary">
			<xs:length value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="hexWord">
		<xs:restriction base="xs:hexBinary">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="listOfHexWord">
		<xs:list itemType="hexWord"/>
	</xs:simpleType>
	
	<xs:simpleType name="listOfAirProtocols">
		<xs:list itemType="llrp:AirProtocols"/>
	</xs:simpleType>

	<!-- Message top-level elements -->
	<xs:element name="KEEPALIVE" type="llrp:KEEPALIVE"/>
	<xs:element name="CLOSE_CONNECTION" type="llrp:CLOSE_CONNECTION"/>
	<xs:element name="GET_READER_CONFIG" type="llrp:GET_READER_CONFIG"/>
	<xs:element name="DISABLE_ACCESSSPEC" type="llrp:DISABLE_ACCESSSPEC"/>
	<xs:element name="SET_READER_CONFIG" type="llrp:SET_READER_CONFIG"/>
	<xs:element name="ERROR_MESSAGE" type="llrp:ERROR_MESSAGE"/>
	<xs:element name="GET_READER_CAPABILITIES" type="llrp:GET_READER_CAPABILITIES"/>
	<xs:element name="CLOSE_CONNECTION_RESPONSE" type="llrp:CLOSE_CONNECTION_RESPONSE"/>
	<xs:element name="CUSTOM_MESSAGE" type="llrp:CUSTOM_MESSAGE"/>
	<xs:element name="ENABLE_ACCESSSPEC_RESPONSE" type="llrp:ENABLE_ACCESSSPEC_RESPONSE"/>
	<xs:element name="DELETE_ROSPEC" type="llrp:DELETE_ROSPEC"/>
	<xs:element name="RO_ACCESS_REPORT" type="llrp:RO_ACCESS_REPORT"/>
	<xs:element name="GET_ROSPECS_RESPONSE" type="llrp:GET_ROSPECS_RESPONSE"/>
	<xs:element name="DELETE_ACCESSSPEC" type="llrp:DELETE_ACCESSSPEC"/>
	<xs:element name="DELETE_ACCESSSPEC_RESPONSE" type="llrp:DELETE_ACCESSSPEC_RESPONSE"/>
	<xs:element name="GET_READER_CAPABILITIES_RESPONSE" type="llrp:GET_READER_CAPABILITIES_RESPONSE"/>
	<xs:element name="SET_READER_CONFIG_RESPONSE" type="llrp:SET_READER_CONFIG_RESPONSE"/>
	<xs:element name="ADD_ACCESSSPEC_RESPONSE" type="llrp:ADD_ACCESSSPEC_RESPONSE"/>
	<xs:element name="DISABLE_ACCESSSPEC_RESPONSE" type="llrp:DISABLE_ACCESSSPEC_RESPONSE"/>
	<xs:element name="GET_ACCESSSPECS_RESPONSE" type="llrp:GET_ACCESSSPECS_RESPONSE"/>
	<xs:element name="ADD_ACCESSSPEC" type="llrp:ADD_ACCESSSPEC"/>
	<xs:element name="READER_EVENT_NOTIFICATION" type="llrp:READER_EVENT_NOTIFICATION"/>
	<xs:element name="ADD_ROSPEC" type="llrp:ADD_ROSPEC"/>
	<xs:element name="DELETE_ROSPEC_RESPONSE" type="llrp:DELETE_ROSPEC_RESPONSE"/>
	<xs:element name="STOP_ROSPEC_RESPONSE" type="llrp:STOP_ROSPEC_RESPONSE"/>
	<xs:element name="ENABLE_ROSPEC_RESPONSE" type="llrp:ENABLE_ROSPEC_RESPONSE"/>
	<xs:element name="START_ROSPEC" type="llrp:START_ROSPEC"/>
	<xs:element name="ENABLE_ACCESSSPEC" type="llrp:ENABLE_ACCESSSPEC"/>
	<xs:element name="DISABLE_ROSPEC_RESPONSE" type="llrp:DISABLE_ROSPEC_RESPONSE"/>
	<xs:element name="KEEPALIVE_ACK" type="llrp:KEEPALIVE_ACK"/>
	<xs:element name="ENABLE_EVENTS_AND_REPORTS" type="llrp:ENABLE_EVENTS_AND_REPORTS"/>
	<xs:element name="GET_ROSPECS" type="llrp:GET_ROSPECS"/>
	<xs:element name="ADD_ROSPEC_RESPONSE" type="llrp:ADD_ROSPEC_RESPONSE"/>
	<xs:element name="GET_READER_CONFIG_RESPONSE" type="llrp:GET_READER_CONFIG_RESPONSE"/>
	<xs:element name="STOP_ROSPEC" type="llrp:STOP_ROSPEC"/>
	<xs:element name="ENABLE_ROSPEC" type="llrp:ENABLE_ROSPEC"/>
	<xs:element name="START_ROSPEC_RESPONSE" type="llrp:START_ROSPEC_RESPONSE"/>
	<xs:element name="GET_REPORT" type="llrp:GET_REPORT"/>
	<xs:element name="DISABLE_ROSPEC" type="llrp:DISABLE_ROSPEC"/>
	<xs:element name="GET_ACCESSSPECS" type="llrp:GET_ACCESSSPECS"/>

	<!-- Generate parameter lists -->

	<xs:group name="extensionPoint">
		<xs:choice>
			<xs:element name="Custom" type="llrp:Custom" minOccurs="0" maxOccurs="unbounded" />
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:group>

	<xs:complexType name="KEEPALIVE">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="CLOSE_CONNECTION">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>

	<xs:complexType name="GET_READER_CONFIG">
		<xs:sequence>
			<xs:element name="AntennaID" type="xs:unsignedShort" />
			<xs:element name="RequestedData" type="llrp:GetReaderConfigRequestedData" />
			<xs:element name="GPIPortNum" type="xs:unsignedShort" />
			<xs:element name="GPOPortNum" type="xs:unsignedShort" />
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>

	<xs:complexType name="DISABLE_ACCESSSPEC">
		<xs:sequence>
			<xs:element name="AccessSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="SET_READER_CONFIG">
		<xs:sequence>
			<xs:element name="ResetToFactoryDefault" type="xs:boolean" />
			<xs:element name="ReaderEventNotificationSpec" type="llrp:ReaderEventNotificationSpec" minOccurs="0" />
			<xs:element name="AntennaProperties" type="llrp:AntennaProperties" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="AntennaConfiguration" type="llrp:AntennaConfiguration" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="ROReportSpec" type="llrp:ROReportSpec" minOccurs="0" />
			<xs:element name="AccessReportSpec" type="llrp:AccessReportSpec" minOccurs="0" />
			<xs:element name="KeepaliveSpec" type="llrp:KeepaliveSpec" minOccurs="0" />
			<xs:element name="GPOWriteData" type="llrp:GPOWriteData" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="GPIPortCurrentState" type="llrp:GPIPortCurrentState" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="EventsAndReports" type="llrp:EventsAndReports" minOccurs="0" />
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ERROR_MESSAGE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_READER_CAPABILITIES">
		<xs:sequence>
			<xs:element name="RequestedData" type="llrp:GetReaderCapabilitiesRequestedData" />
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="CLOSE_CONNECTION_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="CUSTOM_MESSAGE">
		<xs:sequence>
			<xs:element name="VendorIdentifier" type="xs:unsignedInt" />
			<xs:element name="MessageSubtype" type="xs:unsignedByte" />
			<xs:element name="Data" type="xs:hexBinary" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ENABLE_ACCESSSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DELETE_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="RO_ACCESS_REPORT">
		<xs:sequence>
			<xs:element name="TagReportData" type="llrp:TagReportData" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="RFSurveyReportData" type="llrp:RFSurveyReportData" minOccurs="0" maxOccurs="unbounded" />
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_ROSPECS_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
			<xs:element name="ROSpec" type="llrp:ROSpec" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DELETE_ACCESSSPEC">
		<xs:sequence>
			<xs:element name="AccessSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DELETE_ACCESSSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_READER_CAPABILITIES_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
			<xs:element name="GeneralDeviceCapabilities" type="llrp:GeneralDeviceCapabilities" minOccurs="0" />
			<xs:element name="LLRPCapabilities" type="llrp:LLRPCapabilities" minOccurs="0" />
			<xs:element name="RegulatoryCapabilities" type="llrp:RegulatoryCapabilities" minOccurs="0" />
			<xs:choice minOccurs="0" >
				<xs:element name="C1G2LLRPCapabilities" type="llrp:C1G2LLRPCapabilities"/>
			</xs:choice>
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="SET_READER_CONFIG_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ADD_ACCESSSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DISABLE_ACCESSSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_ACCESSSPECS_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
			<xs:element name="AccessSpec" type="llrp:AccessSpec" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ADD_ACCESSSPEC">
		<xs:sequence>
			<xs:element name="AccessSpec" type="llrp:AccessSpec" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="READER_EVENT_NOTIFICATION">
		<xs:sequence>
			<xs:element name="ReaderEventNotificationData" type="llrp:ReaderEventNotificationData" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ADD_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpec" type="llrp:ROSpec" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DELETE_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="STOP_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ENABLE_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="START_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ENABLE_ACCESSSPEC">
		<xs:sequence>
			<xs:element name="AccessSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DISABLE_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="KEEPALIVE_ACK">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ENABLE_EVENTS_AND_REPORTS">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_ROSPECS">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ADD_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_READER_CONFIG_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
			<xs:element name="Identification" type="llrp:Identification" minOccurs="0" />
			<xs:element name="AntennaProperties" type="llrp:AntennaProperties" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="AntennaConfiguration" type="llrp:AntennaConfiguration" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="ReaderEventNotificationSpec" type="llrp:ReaderEventNotificationSpec" minOccurs="0" />
			<xs:element name="ROReportSpec" type="llrp:ROReportSpec" minOccurs="0" />
			<xs:element name="AccessReportSpec" type="llrp:AccessReportSpec" minOccurs="0" />
			<xs:element name="LLRPConfigurationStateValue" type="llrp:LLRPConfigurationStateValue" minOccurs="0" />
			<xs:element name="KeepaliveSpec" type="llrp:KeepaliveSpec" minOccurs="0" />
			<xs:element name="GPIPortCurrentState" type="llrp:GPIPortCurrentState" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="GPOWriteData" type="llrp:GPOWriteData" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="EventsAndReports" type="llrp:EventsAndReports" minOccurs="0" />
			<xs:group ref="llrp:extensionPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="STOP_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="ENABLE_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="START_ROSPEC_RESPONSE">
		<xs:sequence>
			<xs:element name="LLRPStatus" type="llrp:LLRPStatus" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_REPORT">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="DISABLE_ROSPEC">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="GET_ACCESSSPECS">
		<xs:attributeGroup ref="llrp:headerAttrs"/>
	</xs:complexType>


	<xs:complexType name="Identification">
		<xs:sequence>
			<xs:element name="IDType" type="llrp:IdentificationType" />
			<xs:element name="ReaderID" type="xs:hexBinary" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:paramAttrs"/>
	</xs:complexType>


	<xs:complexType name="ChannelIndex">
		<xs:sequence>
			<xs:element name="ChannelIndex" type="xs:unsignedShort" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:paramAttrs"/>
	</xs:complexType>


	<xs:complexType name="ROSpecStartTrigger">
		<xs:sequence>
			<xs:element name="ROSpecStartTriggerType" type="llrp:ROSpecStartTriggerType" />
			<xs:element name="PeriodicTriggerValue" type="llrp:PeriodicTriggerValue" minOccurs="0" />
			<xs:element name="GPITriggerValue" type="llrp:GPITriggerValue" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:paramAttrs"/>
	</xs:complexType>


	<xs:complexType name="ROSpecID">
		<xs:sequence>
			<xs:element name="ROSpecID" type="xs:unsignedInt" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:paramAttrs"/>
	</xs:complexType>


	<xs:complexType name="C1G2BlockEraseOpSpecResult">
		<xs:sequence>
			<xs:element name="Result" type="llrp:C1G2BlockEraseResultType" />
			<xs:element name="OpSpecID" type="xs:unsignedShort" />
		</xs:sequence>
		<xs:attributeGroup ref="llrp:paramAttrs"/>
	</xs:complexType>


	<xs:complexType name="C1G2_PC">
